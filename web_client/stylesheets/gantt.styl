.chart
    font-family 'Droid Sans',sans-serif
    font-size 12px
 
.axis path,.axis line 
    fill none
    stroke #000
    shape-rendering crispEdges
 
.birth 
    fill #33b5e5
    stroke #e7298a
    stroke-width 2px
 
.scan
    fill #669900
    stroke green
    stroke-width 2px

.g-collection-infopage-gantt
    width 100%
    height 600px


